/* pages/checkbox/checkbox.wxss */
.list{
  width: 100%;
  height: 70rpx;
  border-bottom: 1rpx solid #ccc;
  display: flex;
}
.list .list-img{
  width: 50rpx;
  height: 50rpx;
}
.list .list-img image{
  width: 100%;
  height: 100%;
}
.select-list{
  width: 100%;
  margin-top: 300rpx;
  height: 140rpx;
  background: #fff;
  z-index: 99;
  padding-left: 30rpx;
  padding-right: 30rpx;
  border: 1rpx solid #EEEEEE;
  box-shadow: 0 0 20rpx 0 rgba(0,0,0,0.10);
  position: fixed;
  left: 0;
  bottom: 100rpx;
  transition: bottom .5s;
}
.transShow{
  bottom: 0rpx;
}